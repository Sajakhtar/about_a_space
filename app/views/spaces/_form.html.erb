<%= simple_form_for [ @user, @space ], html: { class: "min-vh-100"} do |f| %>
  <%= f.input :space, collection: Space::SPACES %>
  <%= f.input :title %>
  <%= f.input :description %>
  <%= f.input :address, id: "space_address" %>
  <%= f.input :price %>
  <%= f.input :capacity %>
  <%= f.input :wifi %>
  <%= f.input :photos, as: :file, input_html: { multiple: true } %>
  <%= f.button :submit, class: "btn btn-primary w-100" %>
<% end %>
